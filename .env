# .env.example - Template for environment variables
# Copy this to .env and fill in your actual values
# NEVER commit .env to version control!

# OpenAI API Key - Get from https://platform.openai.com/api-keys
OPENAI_API_KEY=sk-proj-your-openai-api-key-here

# Google PageSpeed API Key - Get from https://developers.google.com/speed/docs/insights/v5/get-started
GOOGLE_PAGESPEED_API_KEY=your-google-pagespeed-api-key-here

# Supabase Configuration (if using authentication)
NEXT_PUBLIC_SUPABASE_URL=your-supabase-url-here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key-here

# Optional: Environment
NODE_ENV=development
